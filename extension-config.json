{
  "version": "2.2.0",
  "id": "@shopgate/pdp-native-share",
  "components": [
    {
      "id": "ProductCTASBefore",
      "path": "frontend/portals/ProductCTASBefore",
      "target": "product.ctas.before",
      "type": "portals"
    },
    {
      "id": "StickyButtons",
      "path": "frontend/portals/StickyButtons",
      "target": "product.sticky-buttons.before",
      "type": "portals"
    },
    {
      "id": "NavbarNavigatorCartButtonBefore",
      "path": "frontend/portals/NavbarNavigatorCartButtonBefore",
      "target": "app-bar.cart-button.before",
      "type": "portals"
    },
    {
      "id": "ProductTabletRightColumnCTAS",
      "path": "frontend/portals/ProductTabletRightColumnCTAS",
      "target": "product.tablet.right-column.ctas",
      "type": "portals"
    },
    {
      "id": "en-US",
      "path": "frontend/locale/en-US.json",
      "type": "translations"
    },
    {
      "id": "es-ES",
      "path": "frontend/locale/es-ES.json",
      "type": "translations"
    },
    {
      "id": "de-DE",
      "path": "frontend/locale/de-DE.json",
      "type": "translations"
    },
    {
      "id": "fr-FR",
      "path": "frontend/locale/fr-FR.json",
      "type": "translations"
    },
    {
      "id": "it-IT",
      "path": "frontend/locale/it-IT.json",
      "type": "translations"
    },
    {
      "id": "nl-NL",
      "path": "frontend/locale/nl-NL.json",
      "type": "translations"
    },
    {
      "id": "pt-PT",
      "path": "frontend/locale/pt-PT.json",
      "type": "translations"
    }
  ],
  "steps": [],
  "configuration": {
    "useWithStickyButtons": {
      "type": "admin",
      "destination": "frontend",
      "default": false,
      "params": {
        "type": "checkbox",
        "label": "Set to true to make it compatible with pdp-sticky-buttons extension"
      }
    },
    "iOSIcon": {
      "type": "admin",
      "destination": "frontend",
      "default": "ios",
      "params": {
        "required": true,
        "type": "text",
        "label": "iOS icon style"
      }
    },
    "iOSIconStyles": {
      "type": "admin",
      "destination": "frontend",
      "default": null,
      "params": {
        "type": "json",
        "label": "iOS icon styles"
      }
    },
    "gmdIcon": {
      "type": "admin",
      "destination": "frontend",
      "default": "gmd",
      "params": {
        "required": true,
        "type": "text",
        "label": "Google Material Design icon style"
      }
    }
  }
}
